<Window x:Class="AccountCheckerBase.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AccountCheckerBase"
        mc:Ignorable="d"
        Title="About"
        Icon="BaseIcon.ico"
        Height="285.842" 
        Width="296.046"
        ResizeMode="NoResize"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        Topmost="True"
        Style="{StaticResource CustomToolWindowStyle}">
    <Grid>
        <Label Name="InfoLabel" Content="This is the about page." Margin="0,13,0,204" HorizontalContentAlignment="Center" FontSize="15"/>
        <Label Name="InfoLabel2" Content="Do what ya gotta do here" Margin="0,213,0,7" HorizontalContentAlignment="Center" FontSize="15"/>
        <Ellipse Margin="104,69,104,100" Stroke="#FF1976D2" StrokeThickness="2" ToolTip="Open Link" MouseDown="Ellipse_MouseDown">
            <Ellipse.Fill>
                <ImageBrush ImageSource="/Images/matt101.jpg"/>
            </Ellipse.Fill>
        </Ellipse>
        <Label Name="CrackedLabel" Content="cracked.to/Matt101" Margin="0,156,0,64" HorizontalContentAlignment="Center" FontSize="15"/>
    </Grid>
